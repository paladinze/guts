{
  "name": "cg-shader-noise",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/cg/shader-noise/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/cg/shader-noise",
        "index": "apps/cg/shader-noise/src/index.html",
        "baseHref": "/",
        "main": "apps/cg/shader-noise/src/main.tsx",
        "polyfills": "apps/cg/shader-noise/src/polyfills.ts",
        "tsConfig": "apps/cg/shader-noise/tsconfig.app.json",
        "assets": [
          "apps/cg/shader-noise/src/favicon.ico",
          "apps/cg/shader-noise/src/assets"
        ],
        "styles": ["apps/cg/shader-noise/src/styles.css"],
        "scripts": [],
        "webpackConfig": "apps/cg/shader-noise/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/cg/shader-noise/src/environments/environment.ts",
              "with": "apps/cg/shader-noise/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "cg-shader-noise:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "cg-shader-noise:build:development"
        },
        "production": {
          "buildTarget": "cg-shader-noise:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/cg/shader-noise/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/cg/shader-noise"],
      "options": {
        "jestConfig": "apps/cg/shader-noise/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
